{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) RCSOP Launcher",
      "type": "cppdbg",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}",
      "args": [
        "-I", "/home/vd/Documents/FH/BA/working/coloring/input",
        "-T", "azimuth-rcs",
        "-O", "/home/vd/Documents/FH/BA/working/coloring/output",
        "-M", "false"
      ],
      "stopAtEntry": false,
      "cwd": "${command:cmake.launchTargetDirectory}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}